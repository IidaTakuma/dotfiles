    {
        "title": "Fn + Allow Key",
        "rules": [
        {
            "description": "HHKB like Arrow Key in US keybord",
            "manipulators": [
            {
                "type": "basic",
                "from": {
                "key_code": "semicolon",
                "modifiers": {
                    "mandatory": [
                    "fn"
                    ],
                    "optional": [
                    "any"
                    ]
                }
                },
                "to": [
                {
                    "key_code": "left_arrow"
                }
                ]
            },
            {
                "type": "basic",
                "from": {
                "key_code": "quote",
                "modifiers": {
                    "mandatory": [
                    "fn"
                    ],
                    "optional": [
                    "any"
                    ]
                }
                },
                "to": [
                {
                    "key_code": "right_arrow"
                }
                ]
            },
            {
                "type": "basic",
                "from": {
                "key_code": "open_bracket",
                "modifiers": {
                    "mandatory": [
                    "fn"
                    ],
                    "optional": [
                    "any"
                    ]
                }
                },
                "to": [
                {
                    "key_code": "up_arrow"
                }
                ]
            },
            {
                "type": "basic",
                "from": {
                "key_code": "slash",
                "modifiers": {
                    "mandatory": [
                    "fn"
                    ],
                    "optional": [
                    "any"
                    ]
                }
                },
                "to": [
                {
                    "key_code": "down_arrow"
                }
                ]
            }
            ]
        }
        ]
    }
